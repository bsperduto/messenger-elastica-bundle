{
	"name" : "bsperduto/elastica-messenger-bundle",
	"type" : "symfony-bundle",
	"description" : "The bundle adds extra features to FOSElasticaBundle bundle",
	"keywords" : [
		"elasticsearch",
		"elastica",
		"fos",
		"performance"
	],
	"license" : "MIT",
	"require" : {
		"php" : "^7.1|^8",
		"symfony/framework-bundle" : "^4|^5|^6",
		"friendsofsymfony/elastica-bundle" : "^5|^6",
		"symfony/messenger" : "^4|^5|^6"
	},
	"autoload" : {
		"psr-4" : {
			"BSperduto\\ElasticaMessengerBundle\\" : ""
		}
	},
	"minimum-stability" : "stable",
	"require-dev" : {
		"doctrine/doctrine-bundle" : "^1.6",
		"doctrine/orm" : "^2.5"
	}
}
